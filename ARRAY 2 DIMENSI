#include <iostream>
using namespace std;

int main (){
    int nilai[3][3]={
    {20, 50, 80},
    {30, 60, 80},
    {45, 65, 85}
    };

    nilai [1][1]=100;
    int i=nilai[1][1];

    for (int i=0; i<3; i++){
        for (int j=0; j<3; j++){
                cout << "Array nilai indeks ke ["<<i<<"]["<<j<<"}  = " << nilai [i][j] << endl;
        }

    }
    cout << "Nilai Variabel i ke 2 : " << i << endl;
    return 0;

    }

